{
  "name": "finishthisidea",
  "version": "1.0.0",
  "description": "Transform your messy code into production-ready apps with AI",
  "author": "FinishThisIdea Team",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "src/mvp-cleanup-service/frontend",
    "src/mvp-cleanup-service/backend",
    "src/template-engine",
    "src/tinder-ui",
    "src/llm-router",
    "templates/*"
  ],
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev -w backend",
    "dev:frontend": "npm run dev -w frontend",
    "build": "npm run build:all",
    "build:all": "npm run build -w frontend && npm run build -w backend",
    "start": "npm run start:all",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "npm run start -w backend",
    "start:frontend": "npm run start -w frontend",
    "test": "npm run test:all",
    "test:all": "echo 'Tests configured but not yet implemented' && exit 0",
    "test:unit": "npm run test:unit --workspaces --if-present",
    "test:integration": "npm run test:integration --workspaces --if-present",
    "test:e2e": "npm run test:e2e --workspaces --if-present",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "generate:doc": "node scripts/generate-real-content.js",
    "agent:demo": "./test-real-system.sh",
    "agent:batch": "./scripts/agent-coordinator.sh batch-docs",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "setup": "npm install && npm run setup:env && npm run docker:up",
    "setup:env": "cp .env.example .env",
    "generate-service": "node scripts/generate-service.js",
    "list-templates": "node scripts/list-templates.js",
    "validate-template": "node scripts/validate-template.js",
    "create-template": "node scripts/create-template.js",
    "deploy": "node scripts/deploy.js",
    "analyze": "npm run analyze:bundle && npm run analyze:deps",
    "analyze:bundle": "npm run analyze -w frontend",
    "analyze:deps": "npm audit && npm outdated",
    "clean": "npm run clean:all",
    "clean:all": "rm -rf node_modules **/node_modules **/dist **/.next **/coverage",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "docs:index": "tsx src/core/documentation-index.ts",
    "docs:validate": "node scripts/validate-docs.js",
    "docs:status": "node scripts/doc-status.js",
    "docs:missing": "tsx src/core/documentation-index.ts missing",
    "docs:duplicates": "tsx src/core/documentation-index.ts duplicates",
    "orchestrate": "tsx finish-this.ts",
    "finish": "tsx finish-this.ts build"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.13.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ora": "^7.0.1",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finishthisidea/finishthisidea.git"
  },
  "bugs": {
    "url": "https://github.com/finishthisidea/finishthisidea/issues"
  },
  "homepage": "https://finishthisidea.com",
  "keywords": [
    "ai",
    "code-cleanup",
    "developer-tools",
    "automation",
    "refactoring",
    "typescript",
    "react",
    "nodejs"
  ]
}
