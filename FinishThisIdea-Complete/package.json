{
  "name": "finishthisidea-platform",
  "version": "1.0.0",
  "description": "Complete AI Innovation Platform with integrated frontend and backend services",
  "main": "server/soulfra-bridge.js",
  "scripts": {
    "dev": "nodemon server/soulfra-bridge.js",
    "dev:simple": "nodemon server/simple-server.js",
    "dev:cleanup": "tsx watch src/server.ts",
    "dev:platform": "nodemon server/api-gateway.js",
    "build": "tsc",
    "build:production": "NODE_ENV=production npm run build",
    "start": "node dist/unified-server.js",
    "start:cleanup": "tsx src/server.ts",
    "start:mvp": "tsx src/server.ts",
    "start:platform": "node server/soulfra-bridge.js",
    "start:bridge": "node server/soulfra-bridge.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.{ts,js}' --coverageReporters='text' --coverageReporters='lcov' --coverageReporters='html'",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:db:performance": "jest --testPathPattern=performance --runInBand",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "lint:report": "eslint src --ext .ts,.js --format json --output-file eslint-report.json",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "deploy": "npm run build && npm run start:platform",
    "health-check": "curl -f http://localhost:8080/api/health || exit 1",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:performance": "tsx prisma/seed-performance.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "docker:build": "docker build -t finishthisidea .",
    "docker:build:ai-api": "docker build -f Dockerfile.ai-api -t finishthisidea-ai-api .",
    "docker:build:analytics": "docker build -f Dockerfile.analytics -t finishthisidea-analytics .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "monitoring:prometheus": "docker-compose up -d prometheus grafana node-exporter",
    "monitoring:stop": "docker-compose stop prometheus grafana node-exporter",
    "docs:generate": "tsx src/services/documentation.service.ts generate",
    "docs:serve": "npx http-server docs -p 8081",
    "load-test:artillery": "artillery run tests/load-tests/artillery-config.yml",
    "load-test:artillery:quick": "artillery quick --count 10 --num 3 http://localhost:3000/health",
    "load-test:k6": "k6 run tests/load-tests/k6-api-test.js",
    "load-test:k6:smoke": "k6 run --vus 1 --duration 30s tests/load-tests/k6-api-test.js",
    "load-test:all": "npm run load-test:artillery && npm run load-test:k6",
    "perf:test": "npm run load-test:artillery:quick && npm run load-test:k6:smoke",
    "perf:benchmark": "npm run load-test:all",
    "perf:ci": "TARGET_URL=http://localhost:3000 npm run load-test:k6:smoke",
    "clean": "rm -rf dist coverage .nyc_output",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prepare": "npx prisma generate",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@bull-board/api": "^5.10.2",
    "@bull-board/express": "^5.10.2",
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/node": "^9.33.0",
    "@sentry/profiling-node": "^9.33.0",
    "@sentry/tracing": "^7.120.3",
    "@types/bcrypt": "^5.0.2",
    "@types/csurf": "^1.11.5",
    "@types/dompurify": "^3.0.5",
    "@types/express-session": "^1.18.2",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^5.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "analytics-node": "^6.2.0",
    "archiver": "^6.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.11.5",
    "cluster": "^0.7.7",
    "compression": "^1.8.0",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.25.0",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.12",
    "mixpanel": "^0.18.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "p-limit": "^3.1.0",
    "posthog-node": "^4.18.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "response-time": "^2.3.3",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.8.1",
    "stripe": "^14.10.0",
    "swagger-ui-express": "^5.0.1",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "yauzl": "^2.10.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/archiver": "^6.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^2.0.16",
    "@types/unzipper": "^0.10.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.8",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "artillery": "^2.0.21",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "k6": "^0.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai-platform",
    "finishthisidea",
    "innovation",
    "ai-arena",
    "marketplace",
    "chat-ai",
    "billion-dollar-game",
    "agent-builder",
    "game-platform",
    "idea-monetization",
    "ai-battles",
    "developer-tools",
    "saas"
  ],
  "author": "FinishThisIdea Team",
  "license": "MIT"
}
