        "preferredModels": ["gpt-4", "claude-3-opus", "llama2:70b"],
        "systemPrompt": "You are an analytical AI focused on logical reasoning and data-driven insights."
      },
      
      "pixelArt": {
        "description": "For pixel art descriptions and retro game aesthetics",
        "temperature": 0.7,
        "topP": 0.9,
        "frequencyPenalty": 0.3,
        "presencePenalty": 0.1,
        "preferredModels": ["mistral", "claude-3-sonnet"],
        "systemPrompt": "You are a pixel art specialist understanding color palettes, sprites, and retro gaming aesthetics."
      },
      
