{
  "name": "document-generator-mvp",
  "version": "1.0.0",
  "description": "Document Generator MVP - 111 layers compacted into production-ready platform",
  "main": "electron-main.js",
  "homepage": "./",
  "scripts": {
    "setup-and-start": "npm install && npm run test-systems && npm start",
    "start": "node server.js",
    "start:original": "node build.js",
    "start:quick": "node build.js --quick",
    "start:brain": "node build.js --brain-only",
    "start:demo": "node build.js --demo",
    "brain": "node build.js --mode brain",
    "demo": "node web-interface/simple-mvp-demo.js",
    "dev": "node build.js --mode full",
    "build": "./build.sh",
    "test": "node test-mvp.js",
    "test-systems": "node startup-complete.js",
    "start-all": "node startup-complete.js --start-server",
    "flag-system": "node flag-tag-system.js",
    "database-setup": "node database-integration.js",
    "distributed-deploy": "node distributed-deployment-layer.js",
    "template-wrapper": "node template-wrapper.js",
    "deploy": "vercel --prod",
    "deploy:railway": "./deploy-to-railway.sh",
    "deploy:vercel": "./deploy-to-vercel.sh",
    "deploy:docker": "docker build -t soulfra-platform . && docker run -p 3000:3000 soulfra-platform",
    "deploy:all": "./deploy-dns-wormhole.sh",
    "verify": "./verify-deployment.sh",
    "pentest": "./pentest-blame-game.sh",
    "help": "node build.js --help",
    "electron": "electron .",
    "electron-dev": "NODE_ENV=development electron .",
    "electron-build": "electron-builder",
    "electron-dist": "npm run electron-build -- --publish=never",
    "electron-pack": "electron-packager . soulfra-platform --platform=all --arch=x64 --out=electron/dist/",
    "build-all": "npm run electron-build -- --mac --win --linux",
    "build-mac": "npm run electron-build -- --mac",
    "build-win": "npm run electron-build -- --win",
    "build-linux": "npm run electron-build -- --linux"
  },
  "keywords": [
    "document-processing",
    "ai",
    "mvp",
    "automation",
    "compactor"
  ],
  "author": "Document Generator Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^3.0.5",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "electron-packager": "^17.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/document-generator-mvp.git"
  },
  "build": {
    "appId": "com.soulfra.platform",
    "productName": "Soulfra Platform",
    "directories": {
      "output": "electron/dist"
    },
    "files": [
      "**/*",
      "!electron/dist/**/*",
      "!electron/build/**/*",
      "!README-TEMPLATE.md",
      "!*.md",
      "!.env.template"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "electron/assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "electron/assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "electron/assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}