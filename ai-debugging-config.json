{
  "providers": {
    "openai": {
      "enabled": true,
      "priority": 2,
      "rateLimit": {
        "requests": 3000,
        "window": 60000
      },
      "retry": {
        "attempts": 3,
        "backoff": 1000
      }
    },
    "anthropic": {
      "enabled": true,
      "priority": 1,
      "rateLimit": {
        "requests": 1000,
        "window": 60000
      },
      "retry": {
        "attempts": 3,
        "backoff": 2000
      }
    },
    "gemini": {
      "enabled": true,
      "priority": 3,
      "rateLimit": {
        "requests": 60,
        "window": 60000
      }
    },
    "qwen": {
      "enabled": false,
      "priority": 4,
      "endpoint": "https://api.qwen.ai/v1/chat/completions"
    },
    "kimi": {
      "enabled": false,
      "priority": 5,
      "endpoint": "https://api.moonshot.cn/v1/chat/completions"
    },
    "deepseek": {
      "enabled": false,
      "priority": 6,
      "endpoint": "https://api.deepseek.com/v1/chat/completions"
    },
    "ollama": {
      "enabled": true,
      "priority": 0,
      "endpoint": "http://localhost:11434",
      "preferredModels": [
        "mistral",
        "llama2",
        "codellama",
        "mixtral"
      ]
    }
  },
  "taskTypeRouting": {
    "tax-calculation": {
      "preferredProviders": ["anthropic", "openai"],
      "requiredCapabilities": ["financial-analysis", "numerical-accuracy"]
    },
    "business-analysis": {
      "preferredProviders": ["anthropic", "gemini"],
      "requiredCapabilities": ["long-context", "reasoning"]
    },
    "code-generation": {
      "preferredProviders": ["deepseek", "openai", "ollama"],
      "requiredCapabilities": ["code-understanding", "syntax-accuracy"]
    },
    "debugging": {
      "preferredProviders": ["anthropic", "deepseek", "openai"],
      "requiredCapabilities": ["error-analysis", "solution-generation"]
    },
    "wallet-analysis": {
      "preferredProviders": ["openai", "anthropic"],
      "requiredCapabilities": ["blockchain-knowledge", "transaction-analysis"]
    }
  },
  "errorHandling": {
    "maxHandoffAttempts": 5,
    "handoffDelay": 500,
    "errorCategories": {
      "rate_limit": {
        "backoffMultiplier": 2,
        "maxBackoff": 60000,
        "switchProvider": true
      },
      "timeout": {
        "retryWithLongerTimeout": true,
        "timeoutIncrease": 1.5,
        "maxTimeout": 120000
      },
      "auth_error": {
        "disableProvider": true,
        "notifyUser": true
      },
      "server_error": {
        "retry": true,
        "maxRetries": 2,
        "switchProvider": true
      }
    }
  },
  "monitoring": {
    "metricsRetention": {
      "detailed": 86400000,
      "aggregated": 604800000,
      "reports": 2592000000
    },
    "alertThresholds": {
      "providerFailureRate": 0.3,
      "systemFailureRate": 0.5,
      "responseTimeP95": 10000,
      "costPerHour": 10
    }
  },
  "integration": {
    "serviceDiscovery": {
      "enabled": true,
      "endpoint": "http://localhost:9999",
      "registerAs": "AI Debugging Dashboard",
      "serviceType": "ai-orchestration"
    },
    "businessServices": {
      "accountingSystem": "http://localhost:3013",
      "taxIntelligence": "http://localhost:3014",
      "walletManager": "http://localhost:3015"
    },
    "existingServices": {
      "authWormhole": "http://localhost:8888",
      "licenseCompliance": "http://localhost:8889",
      "systemBus": "http://localhost:8899"
    }
  },
  "debuggingFeatures": {
    "captureFullRequests": true,
    "captureFullResponses": true,
    "logTokenUsage": true,
    "trackLatencyBreakdown": true,
    "recordHandoffChains": true,
    "generateDailyReports": true
  },
  "costOptimization": {
    "preferLocalModels": true,
    "cacheResponses": true,
    "cacheTTL": 3600000,
    "maxCostPerRequest": 0.5,
    "dailyBudget": 100
  }
}