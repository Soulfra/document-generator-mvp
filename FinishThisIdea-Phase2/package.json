{
  "name": "finishthisidea-phase2",
  "version": "2.0.0",
  "description": "AI-powered developer service platform - Documentation, API, and Test Generation",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "generate-service": "tsx scripts/generate-service.ts",
    "test:services": "tsx scripts/test-all-services.ts",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
