{
  "view": "public",
  "hideEscapes": true,
  "cleanOutput": true,
  "format": {
    "removeDebugInfo": true,
    "sanitizeOutput": true,
    "escapeHandler": "auto-fix",
    "visibleSections": [
      "summary",
      "publicContent",
      "documentation"
    ],
    "excludePatterns": [
      "debug",
      "private",
      "internal",
      "escape",
      "leak"
    ]
  },
  "transformations": {
    "/>": "FIXED",
    "&gt;": ">",
    "&lt;": "<",
    "&amp;": "&",
    "blue": "default",
    "cursor: pointer": "cursor: default"
  },
  "watermark": {
    "enabled": true,
    "text": "PUBLIC EXPORT",
    "position": "bottom-right"
  }
}