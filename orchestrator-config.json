{
  "version": "1.0.0",
  "description": "Unified configuration for all AI orchestrators and creative systems",
  
  "aiConfig": {
    "defaultProvider": "ollama",
    "fallbackProviders": ["anthropic", "openai", "deepseek"],
    
    "globalDefaults": {
      "temperature": 0.7,
      "maxTokens": 2048,
      "topP": 0.9,
      "frequencyPenalty": 0.3,
      "presencePenalty": 0.1
