    "@bull-board/express": "^5.10.2",
    "@openapitools/openapi-generator-cli": "^2.8.0",
    "@prisma/client": "^5.7.1",
    "@types/express-session": "^1.18.2",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^9.1.6",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^3.1.0",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "socket.io": "^4.6.2",
    "stripe": "^14.10.0",
    "swagger-parser": "^10.0.3",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
