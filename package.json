{
  "name": "document-generator-mvp",
  "version": "1.0.0",
  "description": "Document Generator MVP - 111 layers compacted into production-ready platform with Agentic Browser",
  "main": "document-generator-mvp-compactor.js",
  "scripts": {
    "start": "node document-generator-mvp-compactor.js",
    "dev": "nodemon document-generator-mvp-compactor.js",
    "test": "node test-mvp.js",
    "deploy": "npm run build && railway deploy",
    "build": "echo 'Build complete - MVP ready for production'",
    "agentic-browser": "node agentic-browser-launcher.js",
    "agentic-dev": "NODE_ENV=development node agentic-browser-launcher.js --dev",
    "setup-deps": "node scripts/install-dependencies.js",
    "check-deps": "node scripts/check-dependencies.js",
    "job-server": "node job-server.js",
    "job-server:dev": "nodemon job-server.js"
  },
  "keywords": [
    "document-processing",
    "ai",
    "mvp",
    "automation",
    "compactor"
  ],
  "author": "Document Generator Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^3.0.5",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "ollama": "^0.4.9",
    "pg": "^8.16.3",
    "puppeteer": "^21.6.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soulfra/document-generator-mvp.git"
  },
  "homepage": "https://soulfra.github.io/document-generator-mvp"
