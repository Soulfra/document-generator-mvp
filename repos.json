    },
    {
      "name": "ai-services-hub", 
      "url": "https://github.com/yourusername/ai-services-hub.git",
      "branch": "main",
      "type": "service",
      "domains": ["ai", "llm", "chatbot", "machine-learning", "openai", "anthropic"],
      "widgets": ["ai-chat", "model-selector", "usage-metrics"],
      "apiEndpoints": ["/chat", "/models", "/usage"],
      "description": "AI services and LLM integration hub",
      "status": "active",
      "priority": 2,
      "deploymentUrl": "https://ai-services-hub.railway.app",
      "healthCheckUrl": "/api/health"
    },
    {
      "name": "business-dashboard",
      "url": "https://github.com/yourusername/business-dashboard.git", 
      "branch": "main",
      "type": "frontend",
      "domains": ["analytics", "dashboard", "business", "metrics", "reporting"],
      "widgets": ["metrics-widget", "chart-widget", "kpi-display"],
      "apiEndpoints": ["/analytics", "/metrics", "/reports"],
      "description": "Business analytics and reporting dashboard",
      "status": "active",
      "priority": 3,
      "deploymentUrl": "https://business-dashboard.netlify.app",
      "healthCheckUrl": "/api/status"
    },
    {
      "name": "api-gateway",
      "url": "https://github.com/yourusername/api-gateway.git",
      "branch": "main", 
      "type": "infrastructure",
      "domains": ["api", "gateway", "routing", "authentication", "rate-limiting"],
      "widgets": ["api-explorer", "rate-limit-display", "auth-status"],
      "apiEndpoints": ["/gateway", "/auth", "/rate-limit"],
      "description": "Central API gateway for all services",
      "status": "active",
      "priority": 0,
      "deploymentUrl": "https://api-gateway.heroku.com",
      "healthCheckUrl": "/health"
    },
    {
      "name": "financial-integration",
      "url": "https://github.com/yourusername/financial-integration.git",
      "branch": "main",
      "type": "service", 
      "domains": ["finance", "payments", "billing", "stripe", "accounting"],
      "widgets": ["payment-widget", "billing-summary", "revenue-chart"],
      "apiEndpoints": ["/payments", "/billing", "/revenue"],
      "description": "Financial services and payment processing",
      "status": "active",
      "priority": 4,
      "deploymentUrl": "https://financial-integration.aws.com",
      "healthCheckUrl": "/api/health"
    },
    {
      "name": "user-management",
      "url": "https://github.com/yourusername/user-management.git",
      "branch": "main",
      "type": "service",
      "domains": ["users", "authentication", "authorization", "profiles", "oauth"],
      "widgets": ["user-profile", "login-status", "permission-display"],
      "apiEndpoints": ["/users", "/auth", "/profiles"],
      "description": "User authentication and management system", 
      "status": "active",
      "priority": 1,
      "deploymentUrl": "https://user-management.firebase.app",
      "healthCheckUrl": "/api/health"
    }
  ],
  "routing": {
    "defaultRoute": "api-gateway",
    "expertRouting": true,
    "fallbackBehavior": "proxy",
    "loadBalancing": "round-robin",
    "caching": {
      "enabled": true,
      "ttl": 300,
      "strategies": ["memory", "redis"]
    }
  },
  "deployment": {
    "autoSync": true,
    "webhooksEnabled": true,
    "syncInterval": 300,
    "healthChecks": {
      "enabled": true,
      "interval": 60,
      "timeout": 10
    },
    "rollback": {
      "enabled": true,
      "maxVersions": 5
    }
  },
  "domains": {
    "primary": "yourdomain.com",
    "api": "api.yourdomain.com", 
    "cdn": "cdn.yourdomain.com",
    "widgets": "widgets.yourdomain.com"
  },
  "security": {
    "webhookSecret": "your-webhook-secret-here",
    "apiKeys": {
      "github": "your-github-token-here",
      "deployment": "your-deployment-key-here"
    },
    "rateLimiting": {
      "enabled": true,
      "window": 3600,
      "max": 1000
    },
    "cors": {
      "enabled": true,
      "origins": ["*"],
      "credentials": true
    }
  },
  "monitoring": {
    "enabled": true,
    "services": ["datadog", "prometheus"],
    "alerts": {
      "email": ["admin@yourdomain.com"],
      "slack": "#alerts"
    }
  },
  "analytics": {
    "enabled": true,
    "tracking": ["google-analytics", "mixpanel"],
    "events": ["widget-load", "api-call", "repo-sync"]
  }
}