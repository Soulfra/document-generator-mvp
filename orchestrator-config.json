        "preferredModels": ["mistral", "claude-3-opus", "gpt-4"],
        "systemPrompt": "You are a highly creative AI assistant focused on generating innovative ideas and artistic concepts."
      },
      
      "technical": {
        "description": "For code generation, technical documentation, and precise tasks",
        "temperature": 0.3,
        "topP": 0.8,
        "frequencyPenalty": 0.1,
        "presencePenalty": 0.0,
        "preferredModels": ["codellama", "deepseek-coder", "gpt-4-turbo"],
        "systemPrompt": "You are a precise technical assistant focused on accuracy and best practices."
      },
      
      "narrative": {
        "description": "For story generation, dialogue, and character development",
        "temperature": 0.8,
        "topP": 0.9,
        "frequencyPenalty": 0.4,
        "presencePenalty": 0.2,
        "preferredModels": ["claude-3-sonnet", "gpt-4", "mistral"],
        "systemPrompt": "You are a master storyteller creating engaging narratives and compelling characters."
